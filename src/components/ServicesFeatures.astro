---
import astroConfig from "../../astro.config.mjs";
import Check from "./Icons/Check.astro";

const { tittle } = Astro.props;
const { text } = Astro.props;

---


<div class="flex flex-row gap-4">
    <Check/>
    <div>
        <h2 class="text-xl font-bold">{tittle}</h2>
        <p>{text}</p>
    </div>
</div>